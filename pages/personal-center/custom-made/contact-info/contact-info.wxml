<import src="/common/head/head" />
<template is="head" />

 <import src="/common/template/wetoast/wetoast.wxml"/>
 <template is="wetoast" data="{{...__wetoast__}}"/>

<form bindsubmit="upDataSubmit">
  <view class="box">
    <view class="title">联系方式</view>

    <view class="field">
      <view class="label">真实姓名</view>
      <input name="name" placeholder="请输入您的真实姓名" placeholder-class="placeholder" />
    </view>

    <view class="field">
      <view class="label">手机号</view>
      <input name="telephone" placeholder="请输入您的手机号" placeholder-class="placeholder" maxlength="11"/>
    </view>
  </view>

  <button class="btn-submit" formType="submit">提交</button>
</form>

